  <%= form_for "hello", class: 'form-horizontal' do |f| %>
    <div class="form-group" >
      <%= f.label :username, class: 'row' %>
      <div class= "row">
        <%= f.text_field :username, class: 'form-control' %>
      </div>
    </div>
    <div class= "form-group">
      <%= f.label :password, class: 'row' %>
      <div class= "row">
        <%= f.password_field :password, class: 'form-control' %>
      </div>
    </div>
    <div>
      <%= f.submit "Login", class: 'btn' %>
    </div>
  <% end %>
